<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net6.0</TargetFramework>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
    <Company>Conexus AS</Company>
    <Copyright>Copyright (c) 2022 Tom Lee</Copyright>
    <PackageProjectUrl>https://github.com/NamNguyen09/cx-api-utilities</PackageProjectUrl>
    <RepositoryUrl>https://github.com/NamNguyen09/cx-api-utilities</RepositoryUrl>
    <PackageLicenseFile>LICENSE</PackageLicenseFile>
    <PackageReleaseNotes>Fix cannot override the appsetting variables by environment variables</PackageReleaseNotes>
    <PackageTags>.NET6, JsonConfiguration</PackageTags>
    <Version>1.0.5</Version>
  </PropertyGroup>

  <ItemGroup>
    <None Include="..\LICENSE">
      <Pack>True</Pack>
      <PackagePath>\</PackagePath>
    </None>
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.Extensions.Configuration.Abstractions" Version="7.0.0" />
    <PackageReference Include="Microsoft.Extensions.Configuration.Json" Version="7.0.0" />
  </ItemGroup>

</Project>
